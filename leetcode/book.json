{
    "title": "leetcode刷题题型总结",
    "description": "leetcode刷题题型总结",
    "author": "sharwen",
    "language": "zh-hans",
    "root": ".",
    "plugins": [
        "expandable-chapters-small",
        "expandable-chapters",
        "code",
        "copy-code-button",
        "-lunr", "-search", "search-pro",
        "github",
        "emphasize",
        "splitter",
        "donate",
        "-sharing", "sharing-plus"
    ],

    "pluginsConfig": {
        "github-buttons": {
            "repo": "https://github.com/fantasydreams",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "donate": {
